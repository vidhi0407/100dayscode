Q140: Define a struct with enum Gender and print person's gender.

#include <stdio.h>
#include <string.h>

enum Gender {
    MALE,
    FEMALE,
    OTHER
};

struct Person {
    enum Gender gender;
};

int main() {
    char input[10];
    struct Person p;

    scanf("%s", input);  

    char *value = strchr(input, '=');  
    if (value != NULL) value++;        

    if (strcmp(value, "MALE") == 0)
        p.gender = MALE;
    else if (strcmp(value, "FEMALE") == 0)
        p.gender = FEMALE;
    else if (strcmp(value, "OTHER") == 0)
        p.gender = OTHER;
    else {
        printf("Invalid Gender\n");
        return 0;
    }

    switch (p.gender) {
        case MALE:
            printf("Male\n");
            break;
        case FEMALE:
            printf("Female\n");
            break;
        case OTHER:
            printf("Other\n");
            break;
    }

    return 0;
}
